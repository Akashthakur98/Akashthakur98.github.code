let game= 'cricket'
switch(game){
    case 'throwball':
        console.log('i loved it');
        break;
    case 'cricket':
        console.log('my fav game');
        break;
    case 'football' :
        console.log('very hard game');
        break;
    default :
    console.log("don't mind")
    break;
}
